{
  "expo": {
    "name": "mali",
    "slug": "mali",
    "version": "1.0.0",
    "scheme": "mali",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.belchushka.mali",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1033752769210-oe6ksm0uqai87ns8if9oujle8145jih3"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [],
      "package": "com.belchushka.mali"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": []
  }
}
